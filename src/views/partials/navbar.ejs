<link rel="stylesheet" href="/public/css/navbar.css">

<nav id="navigation">
    <div class="nav-logo">
        <a href="/">
            <img id="brand-logo" src="/public/icons/logo.png" alt="my-logo" width="100" height="100">
        </a>
    </div>
    <div class="nav-navigation">
        <ul class="nav-links">
            <% navLinks.forEach(nav => { %>
                <li class="nav-item">
                    <a href="<%= nav.link %> "><%= nav.title %> </a>
                </li>
            <% }) %>
        </ul>

        <div type="button" class="nav-button-trigger">
            <img src="/public/icons/hamburger-menu.svg" alt="hamburger" height="35px">
        </div>
    </div>
</nav>

<script src="/public/js/navbar.js"></script>